# SCT211-0010/2022
class Calc:
    def __init__(self,a,b):
        self.a=a
        self.b=b
        
    def add(self):
        return self.a+self.b
    def subtract(self):
        return self.a-self.b
    def division(self):
        return self.a/self.b
    def multiplication(self):
        return self.a*self.b



obj1=Calc(20,10) #example object1
obj2=Calc(30,20) #example object2
print(obj1.add())
print(obj2.multiplication())
